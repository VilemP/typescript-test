{
  "name": "TypescriptTest",
  "version": "0.0.0-development",
  "description": "",
  "main": "Car.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/typings install",
    "commit": "git-cz",
    "lint": "tslint ./src/{,**/}*.ts",
    "test": "./node_modules/.bin/mocha -w --opts ./mocha.opts",
    "test:single": "./node_modules/.bin/mocha --opts ./mocha.opts",
    "test:commit": "npm run lint --silent",
    "test:push": "npm run test:single --silent && npm run lint --silent",
    "build": "./node_modules/.bin/webpack",
    "precommit": "npm run test:commit --silent",
    "prepush": "npm run test:push --silent",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.1.3"
  },
  "devDependencies": {
    "@krux/condition-jenkins": "^1.0.1",
    "chai": "^3.5.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "husky": "^0.11.9",
    "mocha": "^3.0.2",
    "reflect-metadata": "^0.1.8",
    "semantic-release": "^4.3.5",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-config-standard": "^1.5.0",
    "tslint-eslint-rules": "^2.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crywolf/typescript-test.git"
  },
  "publishConfig": {
    "registry": "http://jenkins.billboard.intra:4873/"
  },
  "release": {
    "branch": "master",
    "verifyConditions": {
      "path": "./node_modules/@krux/condition-jenkins"
    }
  }
}
